<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="property.model.Property" table="property">
      <id name="propertyID" type="java.lang.Integer" column="propertyid">
        <generator class="increment" />
      </id>
  <property name="type">
     <column name="type" />
  </property>
  <property name="rooms">
    <column name="rooms"/>
  </property>
  <property name="description">
    <column name="description"/>
  </property>
 </class>
</hibernate-mapping>